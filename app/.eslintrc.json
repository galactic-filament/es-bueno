{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            "error",
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "never"
        ],
        "strict": [
            2,
            "global"
        ],
        "prefer-const": [
            2
        ],
        "no-const-assign": [
            2
        ],
        "no-console": "off",
        "eqeqeq": "error",
        "prefer-template": "error"
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions":{
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    }
}